package com.att.it.bsa.mx.service;

import org.springframework.integration.annotation.Gateway;
import com.att.it.bsa.mx.domain.Matter;


/**
 * Gateway interface. Provides facade to clients of the process pipeline
 * Will place all Orders into the 'orders' channel
 *
 */
public interface MatterGateway {

	@Gateway(requestChannel="mattersInputChannel")
	void processMatters(Matter order);

}